<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="TopTextBlockStyle" TargetType="TextBlock">
        <Setter Property="Foreground" Value="#a9a9a9"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="Margin" Value="0"/>
        <Setter Property="FontWeight" Value="DemiBold"/>
        <Setter Property="FontSize" Value="12"/>
    </Style>
    <Style x:Key="MiddleTextBlockStyle" TargetType="TextBlock">
        <Setter Property="Foreground" Value="#9e9e9f"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Margin" Value="0 5 6 0"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="Height" Value="38"/>
        <Setter Property="FontWeight" Value="DemiBold"/>
    </Style>
</ResourceDictionary>